{
  "entryFile": "src/index.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "spec": {
    "outputDirectory": "public",
    "specVersion": 3,
    "securityDefinitions": {
      "api_key": {
        "type": "apiKey",
        "name": "admin_token",
        "in": "header"
      },
      "jwt": {
        "type": "http",
        "scheme": "Bearer",
        "name": "Authorization",
        "in": "header"
      }
    }
  }
}